{
  "compilerOptions": {
    "allowJs": false,
    "module": "CommonJS",
    "target": "ES6",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": true,
    "noImplicitOverride": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "sourceMap": true
  }
}